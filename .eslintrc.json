{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "@typescript-eslint/no-explicit-any": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "@typescript-eslint/no-unused-vars": "error",
    "react/no-unescaped-entities": "off",
    "no-console": "warn"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  }
}
